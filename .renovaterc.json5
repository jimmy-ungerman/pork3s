{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended",
    "docker:enableMajor",
    "helpers:pinGitHubActionDigests",
    "github>jimmy-ungerman/pork3s//.renovate/autoMerge.json5",
    "github>jimmy-ungerman/pork3s//.renovate/changelogs.json5",
    "github>jimmy-ungerman/pork3s//.renovate/customManagers.json5",
    "github>jimmy-ungerman/pork3s//.renovate/grafanaDashboards.json5",
    "github>jimmy-ungerman/pork3s//.renovate/groups.json5",
    "github>jimmy-ungerman/pork3s//.renovate/labels.json5",
    "github>jimmy-ungerman/pork3s//.renovate/overrides.json5",
    "github>jimmy-ungerman/pork3s//.renovate/semanticCommits.json5",
    ":automergeBranch",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":timezone(America/Boise)",
  ],
  dependencyDashboardTitle: "Renovate Dashboard ðŸ¤–",
  suppressNotifications: [
    "prEditedNotification",
    "prIgnoreNotification",
  ],
  ignorePaths: [
    "**/resources/**",
  ],
  flux: {
    managerFilePatterns: ["/\\.yaml(?:\\.j2)?$/"],
  },
  "helm-values": {
    managerFilePatterns: ["/\\.yaml(?:\\.j2)?$/"],
  },
  kubernetes: {
    managerFilePatterns: ["/\\.yaml(?:\\.j2)?$/"],
  },
  "packageRules": [
    {
      "matchPackageNames": [
        "ghcr.io/home-operations/k8s-sidecar"
      ],
      "allowedVersions": "!/^(1\\.30\\.(1[0-9]|[2-9][0-9])|2\\.[01]\\.[0-4])/"
    }
  ]
}